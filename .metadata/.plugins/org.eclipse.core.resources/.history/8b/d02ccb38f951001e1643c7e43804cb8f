package customers;

public class CustomerTset2 {

	public static void main(String[] args) {
		// 부모
		Customer lee = new Customer(1001, "이순신");
		
		int price = 10000;
		
		int cost = lee.calcPrice(price);
		
		System.out.println(lee.getCsutomerName() + "님이 지불한 비용은 " +
		 cost + "원입니다.");
		System.out.println(lee.showCsutomerInfo());
	}

}
