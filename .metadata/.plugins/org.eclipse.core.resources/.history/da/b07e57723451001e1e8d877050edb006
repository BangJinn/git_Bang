package inheritance.person;

public class Person {
	String name;
	int age;
	
	public Person(String name, int age, int companyId) {
		this.name = name;
		this.age = age;
		this.companyId = companyID;
		
	}
	public String getName() {
		return name;
		
	}
	public int getAge() {
		return age;
		
	}
	
}
