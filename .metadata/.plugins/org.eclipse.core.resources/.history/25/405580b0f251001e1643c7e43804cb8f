package customers;

public class VIPCustomer extends Customer {
	private int agentId;		//전문 상담원
	double saleRatio;			//구매 할인율
	
	public VIPCustomer() {
		customerGrade = "VIP";	
		saleRatio = 0.1;		//10%
		bounsRatio = 0.05;		//5%
		
		
		
	} 
	public void setAgentId(int agegtId) {
		this.agentId = agentId;
		
	}
	
	
}
