package inheritance.person.statement;

public class Drink {
	private String name;
	private int price;
	private int count;
	
	public Drink(String name, int price, int count) {
		this.name = name;
		this.price = price;
		this.count = count;
		
		
		
		
	}
	
	public void printTitle() {
		System.out.println("상품명\t가격\t수량\t금액");
		
	}
	
	
	
}
