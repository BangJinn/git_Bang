package methods;

public class MethodTest1 {

	//변수의 선언과 사용
	//함수 정의(선언)과 호출(사용)
	
	
	
	
	
	public static void main(String[] args) {
		sayHello();

	}
	
	//sayHello() 이름의 함수를 정의
	public static void sayhello() {
		System.out.println("안녕하ㅔ요~ 자바");
		
	}
	
	//매개변수가 있는 함수 정의
	public static void sayhello2(String name) {
		System.out.println("안녕하세요~ " + name);
	}
	
	
}
