package kr.or.iei.run;

import kr.or.iei.model.vo.User;

import kr.or.iei.func.Exam;

public class Start {

public static void main(String[] args) {

	Start user1 = new Start();
	
	User[] users = new User[3];
	
	user1.toString();
	user1.toString();
	user1.toString();
	user1.toString();
	user1.toString();
	
	System.out.println("아이디\t비밀번호\t이름\t나이\t전화번호");

	for(int i = 0; i < users.length; i++) {

	System.out.println(users[i].getId()+"\t"+users[i].getPwd()+"\t"+users[i].getName()+"\t"+users[i].getAge()+"\t"+users[i].getPhone());

	}
	
	
	System.out.println("아이디 :" + user1.toString());
	System.out.println("비밀번호 :" + user1.toString());
	System.out.println("이름 :" + user1.toString());
	System.out.println("나이 :" + user1.toString());
	System.out.println("번호 :" + user1.toString());
	
}

}